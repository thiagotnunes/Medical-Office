<h1>Listing telephone_labels</h1>

<table>
  <tr>
    <th>Label</th>
  </tr>

<% for telephone_label in @telephone_labels %>
  <tr>
    <td><%=h telephone_label.label %></td>
    <td><%= link_to 'Show', telephone_label %></td>
    <td><%= link_to 'Edit', edit_telephone_label_path(telephone_label) %></td>
    <td><%= link_to 'Destroy', telephone_label, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New telephone_label', new_telephone_label_path %>
