<% semantic_form_for(@telephone_label) do |f| %>

	<% unless @telephone_label.errors.empty? %>
    <div class="error_title">
      An error has occurred!
    </div>
  <% end %>
	
	<% f.inputs "Telephone Label" do %>
		<%= f.input :label %>
	<% end %>
	
	<div class="footnote_links_buttons">
  	<%= f.submit action %>
		<%= submit_tag "Cancel" %>
	</div>
<% end %>
