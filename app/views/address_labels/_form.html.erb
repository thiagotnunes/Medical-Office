<% semantic_form_for(@address_label) do |f| %>

	<% unless @address_label.errors.empty? %>
    <div class="div_error_title">
      An error has occurred!
    </div>
  <% end %>

	<% f.inputs "Address Label" do %>
  	<%= f.input :label %>
	<% end %>

	<div class="div_footnote_links_buttons">
  	<%= f.submit action %>
		<%= submit_tag "Cancel" %>
	</div>
<% end %>
