<div class="fields">
  <%= f.input :patient_history_label_id, :collection => PatientHistoryLabel.all, :label_method => :label, :include_blank => true %>
  <%= f.input :history %>
  <% if render_remove_button %>
		<div class="div_remove">
      <%= f.hidden_field :_destroy %>
      <%= link_to_remove_fields "Remove history", f %>
    </div>
  <% end %>
</div>
