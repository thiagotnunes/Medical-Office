
<% semantic_form_for resource_name, resource, :url => registration_path(resource_name) do |f| -%>
  <% f.inputs "Sign up" do %>
    <%= f.input :email %>
    <%= f.input :password %>
    <%= f.input :password_confirmation %>
  <% end %>

  <div class="devise_shared_links">
    <%= render :partial => "shared/devise_links" %>
  </div>
  
  <div class="footnote_links_buttons">
    <%= f.submit "Sign up" %>
  </div>
<% end -%>
